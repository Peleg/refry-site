{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "jasmine": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/no-is-mounted": 0,
    "react/no-deprecated": 0,
    "react/prop-types": 0,
    "func-names": 0,
    "arrow-body-style": 0,
    "comma-dangle": [2, "never"],
    "no-unused-expressions": 0,
    "no-console": 0,
    "no-empty": 0,
    "arrow-parens": [2, "always"],
    "no-param-reassign": 0
  }
}
